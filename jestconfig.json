{
  "transformIgnorePatterns": ["<rootDir>/node_modules/(?!(axios)/)"],
  "extensionsToTreatAsEsm": [".ts"],
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "mjs", "json", "node"],
  "testEnvironment": "jsdom"
}
